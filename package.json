{
  "name": "mikucrypter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail -n \"Client,Server\" \"npm run dev:client\" \"npm run dev:server\"",
    "dev:server": "cross-env NODE_ENV=development tsx watch server/index.ts",
    "dev:client": "cross-env NODE_ENV=development vite",
    "clean": "rimraf ./dist",
    "build": "grunt build",
    "build:client": "grunt build_client",
    "build:server": "grunt build_server",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=production node server/dist/index.js"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.6",
    "@tailwindcss/vite": "^4.0.9",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "fs": "^0.0.1-security",
    "lucide-react": "^0.477.0",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
